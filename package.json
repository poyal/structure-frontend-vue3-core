{
  "name": "structure-frontend-vue3-core",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "serve:dev": "vite --mode dev",
    "serve:stg": "vite --mode stage",
    "serve:prod": "vite --mode prod",
    "preview": "vite preview",
    "build:dev": "vite build  --mode dev",
    "build:stg": "vite build  --mode stage",
    "build:prod": "vite build  --mode prod",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "delete-files": "rm -f package-lock.json && rm -rf node_modules && rm -rf dist && rm -f stats.html",
    "npm-cache-clean": "npm cache clean --force",
    "npm-install": "npm i",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test": "vitest --ui",
    "visualizer": "vite-bundle-visualizer --mode prod",
    "pm2:start": "pm2 serve ./dist/ 5173 --spa --name vtm3_frontend",
    "pm2:restart": "pm2 restart vtm3_frontend",
    "pm2:stop": "pm2 stop vtm3_frontend",
    "pm2:delete": "pm2 delete vtm3_frontend"
  },
  "dependencies": {
    "@js-joda/core": "^5.6.5",
    "axios": "^1.9.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "dompurify": "^3.2.6",
    "js-cookie": "^3.0.5",
    "pinia": "^3.0.3",
    "qs": "^6.14.0",
    "reflect-metadata": "^0.2.2",
    "tsyringe": "^4.10.0",
    "vue": "^3.5.16",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.27.2",
    "@babel/preset-typescript": "^7.27.1",
    "@rushstack/eslint-patch": "^1.11.0",
    "@storybook/addon-a11y": "^9.0.9",
    "@storybook/addon-docs": "^9.0.9",
    "@storybook/addon-links": "^9.0.9",
    "@storybook/cli": "^9.0.9",
    "@storybook/vue3-vite": "^9.0.9",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/user-event": "^14.6.1",
    "@testing-library/vue": "^8.1.0",
    "@tsconfig/node20": "^20.1.5",
    "@types/js-cookie": "^3.0.6",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^24.0.1",
    "@vitejs/plugin-vue": "^5.2.4",
    "@vitest/ui": "^3.2.3",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/tsconfig": "^0.7.0",
    "dotenv": "^16.5.0",
    "eslint": "^8.57.1",
    "eslint-plugin-storybook": "^9.0.9",
    "eslint-plugin-vue": "^9.33.0",
    "jsdom": "^26.1.0",
    "prettier": "3.5.3",
    "sass": "^1.89.2",
    "storybook": "^9.0.9",
    "typescript": "^5.8.3",
    "vite": "^6.3.5",
    "vite-bundle-visualizer": "^1.2.1",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.3",
    "vue-tsc": "^2.2.10"
  }
}
